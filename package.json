    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jsdoc-to-markdown": "^5.0.0",
    "prettier": "^1.17.1",
    "rollup": "^1.12.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "size-limit": "^1.3.2"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
